// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id                  Int @id @default(autoincrement())
  username            String
  email               String @unique
  password            String
}

model Comment {
  id                  Int @id @default(autoincrement())
  anime_mal_id        String
  comment             String
  anime_title         String
}

model Rating {
  id                  Int @id @default(autoincrement())
  anime_mal_id        String
  anime_title         String
  rating              Int
}